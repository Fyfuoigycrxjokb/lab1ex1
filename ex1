#include <stdio.h>
#include <stdlib.h>

int main()
{
    int h, r;
    float s,a,v;
    printf("input h=\n");
    scanf ("%d", &h);
    printf ("input r=\n");
    scanf ("%d", &r);
    s=2*3.14*r*h;
    a=2*3.14*r*(r+h);
    v=3.14*r*r*h;
    printf ("s=%.2f", s);
    printf ("a=%.2f", a);
    printf ("v=%.2f", v);
return 0;
}
